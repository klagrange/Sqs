{
  "extends": "tslint-config-airbnb",
  "rules": {
    "import-blacklist": [true, {
      "chai": ["expect"]
    }],
    "max-line-length": {
      "severity": "warning",
      "options": [120]
    },
    "no-default-export": true,
    "only-arrow-functions": [true, "allow-named-functions"],
    "ordered-imports": {
      "severity": "warning"
    },
    "variable-name": false
  }
}
